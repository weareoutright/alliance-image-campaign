{
  "name": "alliance-image-campaign",
  "version": "1.0.0",
  "description": "Alliance for Care at Home - Landing Page Campaign",
  "main": "assets/index.js",
  "scripts": {
    "sass:watch": "sass --watch src/styles.scss:assets/css/styles.css",
    "sass:build": "sass src/styles.scss:assets/css/styles.css",
    "sass:build:prod": "sass --style=compressed src/styles.scss:assets/css/styles.css",
    "serve": "npx http-server -p 8000 -o",
    "dev": "npm run sass:watch & npm run serve"
  },
  "keywords": ["healthcare", "landing-page", "responsive", "scss", "bem"],
  "author": "Alliance for Care at Home",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "sass": "^1.69.0"
  }
}
